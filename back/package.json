{
    "name": "back",
    "version": "1.0.0",
    "description": "Api for Zg SoundBoard",
    "main": "index.js",
    "scripts": {
        "start": "npm run build && npm run watch",
        "build": "npm run build-ts",
        "serve": "nodemon --inspect=0.0.0.0:9229 dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
        "test": "mocha dist/test",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "author": "",
    "license": "ISC",
    "homepage": "https://github.com/Microsoft/TypeScript-Node-Starter#readme",
    "dependencies": {
        "body-parser": "~1.13.2",
        "cookie-parser": "~1.3.5",
        "debug": "~2.2.0",
        "express": "~4.13.1",
        "grunt": "^1.0.1",
        "mongoose": "^4.12.1",
        "morgan": "~1.6.1",
        "multer": "^1.3.0",
        "q": "^1.5.0",
        "serve-favicon": "~2.3.0",
        "typescript": "^2.5.3"
    },
    "devDependencies": {
        "@types/body-parser": "^1.16.1",
        "@types/chai": "^4.0.4",
        "@types/cookie-parser": "^1.3.30",
        "@types/debug": "^0.0.29",
        "@types/express": "^4.0.35",
        "@types/gulp": "^4.0.2",
        "@types/gulp-less": "^0.0.30",
        "@types/gulp-mocha": "^0.0.30",
        "@types/gulp-nodemon": "^0.0.30",
        "@types/marked": "^0.0.28",
        "@types/mocha": "^2.2.43",
        "@types/mongodb": "^2.2.13",
        "@types/mongoose": "^4.7.23",
        "@types/morgan": "^1.7.32",
        "@types/multer": "^1.3.3",
        "@types/node": "^7.0.10",
        "@types/power-assert": "^1.4.29",
        "@types/serve-favicon": "^2.2.28",
        "browser-sync": "^2.18.8",
        "chai": "^4.1.2",
        "concurrently": "^3.5.0",
        "gulp": "^3.9.1",
