{
  "name": "back",
  "version": "1.0.0",
  "description": "Api for Zg SoundBoard",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts",
    "serve": "nodemon --inspect=0.0.0.0:9229 dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "test": "mocha dist/test",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Microsoft/TypeScript-Node-Starter#readme",
  "dependencies": {
    "body-parser": "~1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~3.1.0",
    "express": "~4.16.3",
    "grunt": "^1.0.3",
    "inversify": "^4.5.1",
    "mongoose": "^5.1.6",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "multiparty": "^4.1.3",
    "q": "^1.5.1",
    "request": "^2.83.0",
    "serve-favicon": "~2.5.0",
    "typescript": "~2.7.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/cookie-parser": "^1.3.30",
    "@types/debug": "^0.0.30",
    "@types/express": "^4.16.0",
    "@types/gulp": "^4.0.5",
    "@types/gulp-less": "^0.0.31",
    "@types/gulp-mocha": "^0.0.32",
    "@types/gulp-nodemon": "^0.0.32",
    "@types/marked": "^0.4.0",
    "@types/mocha": "~5.2.0",
    "@types/mongodb": "^3.0.21",
    "@types/mongoose": "^5.0.18",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.3.5",
    "@types/power-assert": "^1.5.0",
